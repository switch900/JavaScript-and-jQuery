var allImages = document.images;
for(let x = 0; x<allImages.length; x++){
	allImages[x].style.border = '4px solid red';
	}

function questionTwo(){
	let today = new Date;
	let futureDate = new Date("Nov 3, 2020");
	let timeToFutureDate = futureDate - today;
	return timeToFutureDate/(60*60*24*1000);
}
